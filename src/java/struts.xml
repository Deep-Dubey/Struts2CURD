<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd"> 

<struts>
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default">
        <action name="registeruser" class="struts2.test.actions.UserAction" method="userExecute">
            <result name="REGISTER">/register.jsp</result>
        </action>
        <action name="report" class="struts2.test.actions.UserAction" method="userReport">
            <result name="REPORT">/report.jsp</result>
        </action>
        <action name="updatedetails" class="struts2.test.actions.UserAction" method="userUpdate">
            <result name="UPDATE">/update.jsp</result>
        </action>
        <action name="deleterecord" class="struts2.test.actions.UserAction" method="userDelete">
            <result name="DELETE">/delete.jsp</result>
        </action>
        <action name="registerproduct" class="struts2.test.actions.ProductAction" method="productExecute">
            <result name="REGISTERPRODUCT">/product.jsp</result>
        </action>
        <action name="productreport" class="struts2.test.actions.ProductAction" method="productReport">
            <result name="REPORTPRODUCT">/productreport.jsp</result>
        </action>
        <action name="updateproductdetails" class="struts2.test.actions.ProductAction" method="productUpdate">
            <result name="UPDATEPRODUCT">/updateproduct.jsp</result>
        </action>
        <action name="deleteproductrecord" class="struts2.test.actions.ProductAction" method="productDelete">
            <result name="DELETEPRODUCT">/deleteproduct.jsp</result>
        </action>
    </package>
</struts>
